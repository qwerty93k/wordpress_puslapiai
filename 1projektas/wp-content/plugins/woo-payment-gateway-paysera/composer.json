{
    "name": "paysera/wordpress-paysera-module",
    "description": "Paysera module for Wordpress",
    "autoload": {
        "psr-4": {
            "Paysera\\": "src/",
            "GuzzleHttp6\\": "vendor/guzzlehttp/guzzle/src/",
            "GuzzleHttp6\\Psr7\\": "vendor/guzzlehttp/psr7/src/",
            "GuzzleHttp6\\Promise\\": "vendor/guzzlehttp/promises/src/"
        },
        "files": [
            "vendor/guzzlehttp/guzzle/src/functions_include.php",
            "vendor/guzzlehttp/psr7/src/functions_include.php",
            "vendor/guzzlehttp/promises/src/functions_include.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "webtopay/libwebtopay": "^1.6",
        "symfony/polyfill-intl-idn": "1.20.0",
        "symfony/polyfill-intl-normalizer": "1.20.0",
        "paysera/lib-delivery-api-merchant-client": "^0.7.0"
    },
    "require-dev": {
        "paysera/lib-php-cs-fixer-config": "^2.2"
    },
    "type": "wordpress-module",
    "version": "3.2.2",
    "license": [
        "GPL-3.0"
    ],
    "minimum-stability": "stable"
}
